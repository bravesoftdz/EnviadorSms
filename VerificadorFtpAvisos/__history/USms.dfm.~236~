  object qryVerificaDestinatario: TADOQuery
    Connection = dbPrincipal
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'Celular'
        DataType = ftString
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      'Select Cnpj,Mensagem  from Enviados'
      'where Celular=:Celular'
      'and Celular is not null'
      'and Enviado = '#39'1'#39' '
      'order by data,hora desc'
      ' limit 1'
      ''
      '')
